{
  "manifest_version": 2,

  "name": "Rubber Ducky",
  "description": "The answer to all of your programming and life's questions.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "icons": {
		 "48": "images/Ducky.png", 
		 "128": "images/Ducky.png" 
  }

}
